{
  "name": "vue-tsx-webpack",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "del -f .husky && husky install && husky add .husky/pre-commit \"npm run lint-staged\"",
    "start": "vue-cli-service serve",
    "build": "run-p type-check build-only",
    "build-only": "vue-cli-service build --no-module",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier -w src",
    "eslint": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "stylelint": "stylelint ./**/*.scss",
    "lint-staged": "npm run format && lint-staged"
  },
  "dependencies": {
    "core-js": "3.27.1",
    "regenerator-runtime": "0.13.11",
    "vue": "3.2.45"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@rushstack/eslint-patch": "1.1.4",
    "@types/node": "18.11.9",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-typescript": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.0",
    "@vue/tsconfig": "0.1.3",
    "cross-env": "7.0.3",
    "del-cli": "5.0.0",
    "eslint": "8.22.0",
    "eslint-plugin-vue": "9.3.0",
    "esno": "0.16.3",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.3",
    "prettier": "2.7.1",
    "sass": "1.57.1",
    "sass-loader": "13.2.0",
    "stylelint": "14.16.1",
    "stylelint-config-standard": "29.0.0",
    "typescript": "4.7.4",
    "vue-tsc": "1.0.9",
    "webpack": "5.0.0"
  }
}
